   "source": [
    "# Write a program to automatically move the mouse cursor every 2 seconds"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import pyautogui as pag\n",
    "import random\n",
    "import time\n",
    "\n",
    "while True:\n",
    "    x_axis = random.randint(600,700)\n",
    "    y_axis = random.randint(200,600)\n",
    "    pag.move(x_axis,y_axis,0.5)\n",
    "    time.sleep(2)"
   ]
  }
